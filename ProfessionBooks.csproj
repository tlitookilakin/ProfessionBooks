<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<LangVersion>Latest</LangVersion>
  </PropertyGroup>

	<PropertyGroup>
		<Version>1.0.0</Version>
		<UpdateKeys></UpdateKeys>
		<Name>Profession Books</Name>
		<Authors>tlitookilakin</Authors>
		<Description>Adds books to grant professions you don't have.</Description>
		<UniqueID>tlitookilakin.ProfessionBooks</UniqueID>
		<Dependencies_VersionBehavior>Update</Dependencies_VersionBehavior>
		<MinimumApiVersion_Behavior>Update</MinimumApiVersion_Behavior>
		<UpdateKeys>Nexus:30767; GitHub:tlitookilakin/ProfessionBooks</UpdateKeys>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Leclair.Stardew.ModManifestBuilder" Version="2.4.1" />
    <PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.3.2" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="SpaceCore">
      <HintPath>$(GamePath)\Mods\SpaceCore\SpaceCore.dll</HintPath>
      <Private>False</Private>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Assets\" />
  </ItemGroup>

</Project>
